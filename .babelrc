{
  "presets": [
    ["env", { "targets": { "node": "7.6" } }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-decorators-legacy"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-inline-elements",
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "closure-elimination"
      ]
    }
  }
}
